{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "generator-cli": {
    "useDocker": true,
    "version": "6.6.0",
    "generators": {
      "hospital": {
        "generatorName": "typescript-fetch",
        "glob": "api/hospital.openapi.yaml",
        "output": "#{cwd}/src/api/hospital",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
